#!/bin/sh

#  //
# // Example tool to illustrate the SVSuite Tool environment
#//


echo "Input Data can be found in: $SVSUITE_INPUT_DIR"
echo "List of Inputs available:"
/bin/ls $SVSUITE_INPUT_DATA
echo "=================================="


echo "CMSSW Version being used is: $SVSUITE_VERSION"

echo "Data Dir is: $SVSUITE_DATA_DIR"
echo "Packages available in the data dir:"
/bin/ls $SVSUITE_DATA_DIR/SVSuite/data/$SVSUITE_VERSION/

echo "================================="



echo "Bin Dir is: $SVSUITE_BIN_DIR"
echo "This is in your path and provides access to the following tools:"
/bin/ls $SVSUITE_BIN_DIR/

echo "================================="

echo "Output Dir is : $SVSUITE_OUTPUT_DIR"
echo "Anything you want to keep as output should be copied to this directory"









